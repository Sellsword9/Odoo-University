<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_report_student"
            model="ir.actions.report">
        <field name="name">Student Report</field>
        <field name="model">university.students</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">university.student_report_template</field>
        <field name="report_file">university.student_report</field>
        <field name="print_report_name">'Student Report - %s' % (object.name)</field>
        <field name="binding_model_id"
               ref="university.model_university_students" />
        <field name="binding_type">report</field>
    </record>
</odoo>